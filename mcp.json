{
    "mcpServers": {
        "deploy-mcp": {
            "command": "python",
            "args": [
                "./mcp_server.py"
            ],
            "transport": "stdio"
        },
        "Bright Data": {
            "env": {
                "API_TOKEN": "<ADD_TOKEN_HERE>"
            },
            "args": [
                "@brightdata/mcp"
            ],
            "command": "npx",
            "transport": "stdio"
        }
    },
    "allowedTools": [
        "deploy-mcp",
        "delete-mcp",
        "list-mcp",
        "search_engine",
        "scrape_as_markdown"
    ]
}